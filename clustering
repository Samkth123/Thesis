    let rightIndex = sortedNums.length - 1;

    for (let i = 0; i < sortedNums.length; i++) {
        let current = sortedNums[i];
        if (current + sortedNums[rightIndex] === target) {
            pairs.add(\`(\${current}, \${sortedNums[rightIndex]})\`);
            rightIndex -= 1;
