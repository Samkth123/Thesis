###### âœ… 1. Function to extract ratings into a DataFrame #######
rwandan_participants = participants  # Just a rename to keep clarity
all_participants = rwandan_participants + dutch_participants + swedish_participants

def extract_country_ratings(participants):
    ratings_data = []
    for p in participants:
        name = p["personal_info"]["name"]
        country = p["personal_info"]["country_of_origin"]
        dev_ratings = p["development_rating"]
        for rated_country, criteria in dev_ratings.items():
            for skill, score in criteria.items():
                ratings_data.append({
                    "participant": name,
                    "origin_country": country,
                    "rated_country": rated_country,
                    "skill": skill,
                    "score": score
                })
    return pd.DataFrame(ratings_data)

########âœ… 2. Function to plot ratings #########
def plot_country_ratings(df, title):
    ax = df.plot(kind="bar", figsize=(10, 6), title=title)
    ax.set_ylabel("Average Score")
    ax.set_xlabel("Rated Country")
    plt.xticks(rotation=45)
    plt.tight_layout()
    plt.show()

########âœ… 3. Code to generate graphs #######

# Combine all participants
all_participants = rwandan_participants + dutch_participants + swedish_participants

# Extract ratings
ratings_df = extract_country_ratings(all_participants)

# Compute average ratings
avg_ratings = ratings_df.groupby(["origin_country", "rated_country", "skill"])["score"].mean().reset_index()

# Create pivot tables for each country
rwanda_ratings = avg_ratings[avg_ratings["origin_country"] == "Rwanda"].pivot(index="rated_country", columns="skill", values="score")
netherlands_ratings = avg_ratings[avg_ratings["origin_country"] == "Netherlands"].pivot(index="rated_country", columns="skill", values="score")
sweden_ratings = avg_ratings[avg_ratings["origin_country"] == "Sweden"].pivot(index="rated_country", columns="skill", values="score")

# Overall average by rated country
overall_avg_ratings = ratings_df.groupby(["rated_country", "skill"])["score"].mean().unstack()

# Plot all
plot_country_ratings(rwanda_ratings, "Ratings by Rwandan Participants")
plot_country_ratings(netherlands_ratings, "Ratings by Dutch Participants")
plot_country_ratings(sweden_ratings, "Ratings by Swedish Participants")
plot_country_ratings(overall_avg_ratings, "Overall Average Ratings of Each Country")
